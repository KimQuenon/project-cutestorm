{% extends 'base.html.twig' %}

{% block title %}
	Add Image To
	{% block form_title %}{% endblock %}
{% endblock %}

{% block body %}
	<div class="container mt-4">
		<h1 class="mb-4">
			Add Image To
			{% block form_heading %}{% endblock %}
		</h1>
		{{ form_start(myForm) }}
		{{ form_widget(myForm) }}
		<button type="submit" class="btn btn-primary mt-4">
			Add
		</button>
		{{ form_end(myForm) }}
		{% block back_button %}{% endblock %}
	</div>
{% endblock %}
